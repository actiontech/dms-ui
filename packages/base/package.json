{
  "name": "base",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "node ../../scripts/writeEnvLocal.mjs feature=sqle,prod_version=ce && vite --port=3020",
    "start:ee": "node ../../scripts/writeEnvLocal.mjs feature=sqle,prod_version=ee &&  vite --port=3020",
    "build": "node ../../scripts/writeEnvLocal.mjs feature=sqle,prod_version=ce && node ../../scripts/getGitVersion.mjs && tsc &&  vite build",
    "build:ee": "node ../../scripts/writeEnvLocal.mjs feature=sqle,prod_version=ee && node ../../scripts/getGitVersion.mjs && tsc &&  vite build",
    "preview": "vite preview",
    "test": "jest --watchAll=true",
    "test:c": "jest --coverage",
    "test:ci": "jest --ci --watchAll=false --coverage --color --silent --testLocationInResults"
  },
  "dependencies": {
    "@actiontech/shared": "workspace:^",
    "babel-preset-react-app": "^10.0.1",
    "sqle": "workspace:^"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
